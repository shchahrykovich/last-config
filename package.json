{
  "name": "last-config",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run --isolate",
    "test:watch": "vitest --isolate",
    "test:coverage": "vitest run --coverage --isolate",
    "db:migrations:apply": "wrangler d1 migrations apply DB --remote",
    "deploy-from-local": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy": "npm run db:migrations:apply && npx opennextjs-cloudflare build && wrangler deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.3.0",
    "@auth/prisma-adapter": "^2.11.1",
    "@opennextjs/cloudflare": "^1.14.0",
    "@prisma/adapter-d1": "^6.19.0",
    "@prisma/client": "^6.19.0",
    "antd": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "lodash": "^4.17.21",
    "luxon": "^3.7.2",
    "next": "16.0.7",
    "next-auth": "^5.0.0-beta.30",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.70",
    "@eslint/eslintrc": "^3",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^16.0.7",
    "prisma": "^6.19.0",
    "typescript": "^5",
    "vitest": "~3.2.0",
    "wrangler": "^4.53.0"
  },
  "cloudflare": {
    "bindings": {
      "AUTH_SECRET": {
        "description": "Secret for auth. `openssl rand -hex 32` - use to generate on mac."
      }
    }
  }
}
